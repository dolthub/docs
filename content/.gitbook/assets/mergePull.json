{
  "openapi": "3.0.0",
  "info": {
    "title": "Merge a pull request",
    "version": "1.0.0",
    "description": "Merge a pull request on Dolthub"
  },
  "servers": [
    {
      "url": "https://www.dolthub.com/api/v1alpha1"
    }
  ],
  
    "paths": {
      "/{owner}/{database}/pulls/{pull_id}/merge": {
        "post": {
          "summary": "Merge a pull request",
          "description": "This endpoint merges a pull request into the destination branch.",
          "parameters": [
            {
              "name": "owner",
              "in": "path",
              "description": "The name of the database owner.",
              "required": true,
              "schema": {
                "type": "string",
                "example":"dolthub"
              }
            },
            {
              "name": "repo",
              "in": "path",
              "description": "The name of the database.",
              "required": true,
              "schema": {
                "type": "string",
                "example":"museum-collections"
              }
            },
            {
              "name": "pull_id",
              "in": "path",
              "description": "The ID of the pull request to merge.",
              "required": true,
              "schema": {
                "type": "string",
                "example":"66"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "The pull request was merged successfully.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Success"
                      },
                      "repository_owner": {
                        "type": "string",
                        "example": "dolthub"
                      },
                      "repository_name": {
                        "type": "string",
                        "example": "museum-collections"
                      },
                      "pull_id": {
                        "type": "string",
                        "example": "66"
                      },
                      "operation_name": {
                        "type": "string",
                        "example": "operations/b09a9221-9dcb-4a15-9ca8-a64656946f12"
                      }
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Bad request. The request was invalid or could not be processed.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "example": "Error"
                      },
                      "message":{
                        "type":"string",
                        "example":"Error merging a pull request."
                      },
                      "repository_owner": {
                        "type": "string",
                        "example": "dolthub"
                      },
                      "repository_name": {
                        "type": "string",
                        "example": "museum-collections"
                      },
                      "pull_id": {
                        "type": "string",
                        "example": "66"
                      }
                    }
                  }
                }
              }
            }
          },
          "security": [
            {
              "APIKeyHeader": []
            }
          ]
        }
      }
    },
    "components": {
      "securitySchemes": {
        "APIKeyHeader": {
          "type": "apiKey",
          "in": "header",
          "name": "authorization"
        }
      }
    }
  }
  
